<p id="notice"><%= notice %></p>

<h1>Imgtowns</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @imgtowns.each do |imgtown| %>
      <tr>
        <td><%= imgtown.name %></td>
        <td><%= imgtown.description %></td>
        <td><%= imgtown.image %></td>
        <td><%= link_to 'Show', imgtown %></td>
        <td><%= link_to 'Edit', edit_imgtown_path(imgtown) %></td>
        <td><%= link_to 'Destroy', imgtown, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Imgtown', new_imgtown_path %>
